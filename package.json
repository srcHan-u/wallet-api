    "babel-loader": "^9.1.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "jsonwebtoken": "^8.5.1",
    "cors": "^2.8.5",
    "axios": "^1.2.0",
    "express-validator": "^6.14.2",
    "bcrypt": "^5.1.0"
    
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^6.7.2",
